[MASTER]
jobs=0

[MESSAGES CONTROL]
disable = fixme,
          no-else-return,
          too-many-arguments,
          too-few-public-methods,
          too-many-locals,
          too-many-instance-attributes,
          no-member,
          unnecessary-pass,
          import-outside-toplevel,
          no-else-continue,
          similarities,
          import-error


[FORMAT]
max-line-length = 120

[BASIC]
good-names = i,
             j,
             k,
             o,
             x
